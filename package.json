{
  "name": "homepage",
  "version": "1.0.0",
  "private": true,
  "description": "Lolipop's homepage and static blog system, inspired by Priority Vision theme Thesis",
  "author": "LolipopJ",
  "keywords": [
    "gatsby",
    "homepage",
    "blog"
  ],
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "prepare": "husky",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint --fix **/*.{js,ts,jsx,tsx}",
    "prettier": "prettier --write .",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{md,mdx,json,css}": [
