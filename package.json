{
  "name": "homepage",
  "version": "1.0.0",
  "private": true,
  "description": "Lolipop's homepage and static blog system, inspired by Priority Vision theme Thesis",
  "author": "LolipopJ",
  "keywords": [
    "gatsby",
    "homepage",
    "blog"
  ],
  "engines": {
    "node": ">= 24",
    "pnpm": ">= 9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "npm run prettier && npm run eslint",
    "eslint": "eslint --fix **/*.{js,ts,jsx,tsx}",
    "prettier": "prettier --write .",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{md,mdx,json,css}": [
      "prettier --write"
    ],
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@fancyapps/ui": "^6.1.7",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@react-spring/web": "^10.0.3",
    "@types/lodash-es": "^4.17.12",
    "algoliasearch": "^5.46.3",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "gatsby": "^5.15.0",
    "gatsby-plugin-algolia": "^1.0.3",
    "gatsby-plugin-feed": "^5.15.0",
    "gatsby-plugin-image": "^3.15.0",
    "gatsby-plugin-layout": "^4.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-mdx": "^5.15.0",
    "gatsby-plugin-postcss": "^6.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-remark-copy-linked-files": "^6.15.0",
    "gatsby-remark-images": "^7.15.0",
    "gatsby-remark-prismjs": "^7.15.0",
    "gatsby-remark-responsive-iframe": "^6.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "gitalk-react": "^1.0.0-beta.8",
    "heti": "^0.9.6",
    "lodash-es": "^4.17.22",
    "nprogress": "^0.2.0",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-instantsearch": "^7.22.1",
    "remark-gfm": "^1.0.0",
    "rich-presence-react": "^1.0.0-beta.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/node": "^24.10.9",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-nesting": "^14.0.0",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^8.57.1"
  }
}
